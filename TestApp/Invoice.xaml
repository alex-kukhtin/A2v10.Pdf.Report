<Report xmlns="clr-namespace:A2v10.Pdf.Xaml;assembly=A2v10.Pdf.Xaml"
		Title="{Bind Expression='Рахунок №{Document.Name} від {FormatDate(Document.Date)}'}">
	<Column>
		<Table ItemsSource="{Bind Document.Rows}">
			<Table.Columns>
				<TableColumn />
				<TableColumn />
				<TableColumn />
			</Table.Columns>
			<Table.Header>
				<TableRow>
					<TableCell Content="{Bind Qty}"/>
					<TableCell Content="{Bind Price}"/>
					<TableCell Content="{Bind Sum}"/>
				</TableRow>
			</Table.Header>
			<TableRow>
				
			</TableRow>
		</Table>
		<Table>
			
		</Table>
	</Column>
</Report>
