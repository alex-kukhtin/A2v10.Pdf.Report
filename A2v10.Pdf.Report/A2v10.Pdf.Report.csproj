<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netframework4.8</TargetFramework>
		<Version>10.0.7980</Version>
		<LangVersion>10</LangVersion>
		<Nullable>enable</Nullable>
		<RootNamespace>A2v10.Pdf.Report</RootNamespace>
		<Product>A2v10</Product>
		<PackageId>A2v10.Pdf.Report</PackageId>
		<Authors>Oleksandr Kukthin</Authors>
		<Company>Oleksandr Kukthin</Company>
		<Description>A2v10 PDF builder</Description>
		<Copyright>Copyright © 2022-2025 Oleksandr Kukthin. All rights reserved.</Copyright>
		<RepositoryType>git</RepositoryType>
		<RepositoryUrl>https://github.com/alex-kukhtin/A2v10.Pdf.Report</RepositoryUrl>
		<PackageTags>A2v10 Platform Pdf</PackageTags>
	</PropertyGroup>

	<ItemGroup>
	  <Content Include="A2v10.Pdf.Report.nuspec" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Jint" Version="4.2.2" />
		<PackageReference Include="QRCoder" Version="1.6.0" />
		<PackageReference Include="QuestPDF" Version="2023.12.6" />
		<PackageReference Include="System.Buffers" Version="4.6.1" />
		<PackageReference Include="System.Memory" Version="4.6.3" />
		<PackageReference Include="System.Numerics.Vectors" Version="4.6.1" />
		<PackageReference Include="System.Runtime.CompilerServices.Unsafe" Version="6.1.2" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\A2v10\Shared\A2v10.Infrastructure\A2v10.Infrastructure.csproj" />
		<ProjectReference Include="..\A2v10.Xaml.Report\A2v10.Xaml.Report.csproj" />
	</ItemGroup>
</Project>
